{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host={DB_HOST};Port={DB_PORT};Database={DB_NAME};Username={DB_USER};Password={DB_PASSWORD};Trust Server Certificate=true;"
  },
  "JwtSettings": {
    "SecretKey": "SUPER_SECRET_KEY",
    "Issuer": "CLYPass",
    "Audience": "CLYPassUsers",
    "ExpiryMinutes": 60
  },
  "AllowedHosts": "*"
}
